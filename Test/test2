def isPalindromeString(s: str):
    """
    迭代实现
    """
    start = 0
    end = len(s) - 1
    while start < end:
        if s[start] != s[end]:
            return False
        start += 1
        end -= 1
    return True


def isPalindromeString(s: str):
    if len(s) <= 1:
        return True
    return s[0] == s[-1] and isPalindromeString(s[1:-1])


print(isPalindromeString('1234567654321'))
